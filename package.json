    "@iconify-json/heroicons": "^1.2.2",
    "@notionhq/client": "^5.0.0",
    "@playform/compress": "^0.2.0",
    "astro": "^5.13.6",
    "astro-color-scheme": "^1.1.5",
    "astro-icon": "^1.1.5",
    "astro-pure": "^1.3.4",
    "astro-seo": "^0.8.4",
    "katex": "^0.16.22",

  "type": "module",
  "version": "0.0.1",
  "packageManager": "npm@10",
  "engines": {
    "node": ">=18.17 <21 || >=22"
  },
  "scripts": {
    "clean": "rimraf dist .astro \"node_modules/.vite\"",
    "build": "astro build",
